{
    "presets": [
        "es2015",
        "react",
        "stage-0",
        "react-native"
    ],
    "plugins": [
        "transform-runtime",
        "transform-decorators-legacy",
        "transform-class-properties",
        [
            "import",
            {
                "libraryName": "antd",
                "style": true
            }
        ]
    ],
    "comments": false
}